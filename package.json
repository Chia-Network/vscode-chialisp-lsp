{
    "name": "chialisp-lsp-client",
    "displayName": "chialisp-lsp-client",
    "description": "LSP client for chialisp",
    "publisher": "prozacchiwawa",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.70.0"
    },
    "categories": [
        "Other"
    ],
    "repository": "https://github.com/prozacchiwawa/vscode-chialisp-lsp",
    "activationEvents": [
        "onLanguage:chialisp"
    ],
    "contributes": {
        "languages": [
            {
                "id": "chialisp",
                "aliases": [
                    "Chialisp",
                    "chialisp"
                ],
                "extensions": [
                    ".cl",
                    ".clvm",
                    ".clib",
                    ".clinc"
                ],
                "configuration": "./language-configuration.json"
            }
        ]
    },
    "configuration": {
        "type": "object",
        "title": "Chialisp",
        "properties": {
        },
        "commands": []
    },
    "main": "./out/extension.js",
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "pretest": "npm run compile && npm run lint",
        "lint": "eslint src --ext ts",
        "test": "node ./out/test/runTest.js"
    },
    "dependencies": {
        "vscode-languageclient": "^7.0.0"
    },
    "devDependencies": {
        "@types/glob": "^7.2.0",
        "@types/mocha": "^9.1.1",
        "@types/node": "16.x",
        "@types/vscode": "^1.70.0",
        "@typescript-eslint/eslint-plugin": "^5.31.0",
        "@typescript-eslint/parser": "^5.31.0",
        "@vscode/test-electron": "^2.1.5",
        "eslint": "^8.20.0",
        "glob": "^8.0.3",
        "mocha": "^10.0.0",
        "typescript": "^4.7.4"
    }
}
